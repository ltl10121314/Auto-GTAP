      sum(s,REG, MTAX(i,s,r)*[0.01*EVSCALFACT(r)]*[qxs(i,s,r) - pop(r)]));

Variable (linear,change) (all,i,TRAD_COMM)(all,s,REG)(all,r,REG)
    CNTqimisr(i,s,r)
